From 6a00342a3264679fd1700177074678efd279fae6 Mon Sep 17 00:00:00 2001
From: William Douglas <william.douglas@intel.com>
Date: Fri, 8 Feb 2019 19:08:43 -0800
Subject: [PATCH] Switch to PySocks

Signed-off-by: Patrick McCarty <patrick.mccarty@intel.com>
---
 gcs_oauth2_boto_plugin.egg-info/requires.txt | 2 +-
 setup.py                                     | 4 +---
 2 files changed, 2 insertions(+), 4 deletions(-)

diff --git a/gcs_oauth2_boto_plugin.egg-info/requires.txt b/gcs_oauth2_boto_plugin.egg-info/requires.txt
index 84cce8e..bb64006 100644
--- a/gcs_oauth2_boto_plugin.egg-info/requires.txt
+++ b/gcs_oauth2_boto_plugin.egg-info/requires.txt
@@ -3,7 +3,7 @@ google-reauth>=0.1.0
 httplib2>=0.8
 oauth2client>=2.2.0
 pyOpenSSL>=0.13
-SocksiPy-branch==1.01
+PySocks>=1.0.0
 retry_decorator>=1.0.0
 six>=1.6.1
 
diff --git a/setup.py b/setup.py
index 9b329c2..acc4b09 100644
--- a/setup.py
+++ b/setup.py
@@ -34,9 +34,7 @@ requires = [
     'httplib2>=0.8',
     'oauth2client>=2.2.0',
     'pyOpenSSL>=0.13',
-    # Not using 1.02 because of:
-    #   https://code.google.com/p/socksipy-branch/issues/detail?id=3
-    'SocksiPy-branch==1.01',
+    'PySocks>=1.0.0',
     'retry_decorator>=1.0.0',
     'six>=1.6.1'
 ]
